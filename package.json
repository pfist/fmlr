{
  "name": "fmlr",
  "description": "Accelerated theme development for Ghost",
  "version": "0.0.0",
  "author": "Nick Pfisterer",
  "bugs": "https://github.com/polymoon/fmlr/issues",
  "homepage": "https://github.com/polymoon/fmlr",
  "keywords": [
    "ghost",
    "theme",
    "cli"
  ],
  "license": "MIT",
  "repository": "polymoon/fmlr",
  "engines": {
    "node": ">=10.0.0"
  },
  "bin": {
    "fmlr": "./bin/run"
  },
  "files": [
    "/bin",
    "/src"
  ],
  "main": "./src/index.js",
  "dependencies": {
    "@oclif/command": "^1.5.18",
    "@oclif/config": "^1.13.2",
    "@oclif/plugin-help": "^2.2.0",
    "autoprefixer": "^9.6.1",
    "browser-sync": "^2.26.7",
    "cli-ux": "^5.3.1",
    "download-git-repo": "^2.0.0",
    "fs-extra": "^8.1.0",
    "globby": "^10.0.1",
    "gulp": "^4.0.2",
    "gulp-postcss": "^8.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-zip": "^5.0.0"
  },
  "oclif": {
    "commands": "./src/commands",
    "bin": "fmlr",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "maintained node versions",
    "not dead"
  ]
}
